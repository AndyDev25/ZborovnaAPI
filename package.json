    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "^16 || ^15 || ^14"
  }
}
