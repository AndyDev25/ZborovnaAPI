{
  "name": "zborovna-api",
  "version": "0.0.1",
  "description": "Simple API for zborovna",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "ts-node-dev --respawn index.ts",
    "dev": "nodemon --exec npx ts-node index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect index.ts"
  },
  "keywords": [
    "API, education, node, zborovna, school, teachers"
  ],
  "author": "AndyDev",
  "license": "MIT",
